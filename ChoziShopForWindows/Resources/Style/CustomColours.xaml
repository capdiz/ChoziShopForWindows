    <SolidColorBrush x:Key="GrayWhiteBrush" Color="{DynamicResource GrayWhiteColor}" />
    <SolidColorBrush x:Key="Teal800Brush" Color="{DynamicResource Teal800}" />
    <SolidColorBrush x:Key="Cyan900Brush" Color="{DynamicResource Cyan900}" />
