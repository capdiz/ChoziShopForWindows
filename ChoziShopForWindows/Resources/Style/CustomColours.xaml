    <SolidColorBrush x:Key="BlueGray900Brush" Color="{DynamicResource BlueGray900}"/>
    <SolidColorBrush x:Key="Green50Brush" Color="{DynamicResource Green50}" />
    <SolidColorBrush x:Key="Lime300Brush" Color="{DynamicResource Lime300}" />
    <SolidColorBrush x:Key="Lime400Brush" Color="{DynamicResource Lime400}" />
</ResourceDictionary>