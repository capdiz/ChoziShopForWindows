    <Color x:Key="Lime400">#D4E157</Color>
    <SolidColorBrush x:Key="DefaultGroupBoxHeaderBrush" Color="{DynamicResource DefaultHeaderColor}"/>
    <SolidColorBrush x:Key="SalesGroupBoxBrush" Color="{DynamicResource SalesGroupBoxColor}" />
    <SolidColorBrush x:Key="TotalAmountHeaderBrush" Color="{DynamicResource AmountHeader}" />
    <SolidColorBrush x:Key="OrderProgressHeaderBrush" Color="{DynamicResource OrderProgressHeader}" />
    <SolidColorBrush x:Key="InProgressColorBrush" Color="{DynamicResource InProgressColor}" />
    <SolidColorBrush x:Key="EasyCheckoutBrush" Color="{DynamicResource EasyPosOrderColor}" />
    <SolidColorBrush x:Key="SearchBoxColorBrush" Color="{DynamicResource SearchBoxColor}" />
    <SolidColorBrush x:Key="SearchFontBrush" Color="{DynamicResource SearchFontColor}" />
    <SolidColorBrush x:Key="ProgressBarBrush" Color="{DynamicResource ProgressBarTextColor}" />
    <SolidColorBrush x:Key="BaseTextBrush" Color="{DynamicResource BaseTextColor}" />
